<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <changeSet id="20230621-1-1" author="SGagarinov" logicalFilePath="db.20230621-1.create_files.xml">
        <createSequence sequenceName="files_seq_id" incrementBy="1" startValue="1" />
    </changeSet>
    <changeSet id="20230621-1-2" author="SGagarinov" logicalFilePath="db.20230621-1.create_files.xml">
        <createTable tableName="files">
            <column name="id" type="bigint" defaultValueSequenceNext="files_seq_id">
                <constraints primaryKey="true" nullable="false" primaryKeyName="files_id"/>
            </column>
            <column name="name" type="varchar(500)">
                <constraints nullable="false" />
            </column>
            <column name="size" type="bigint">
                <constraints nullable="false" />
            </column>
            <column name="description" type="text" />
            <column name="create_date" type="timestamp">
                <constraints nullable="false" />
            </column>
            <column name="create_user" type="varchar(255)" />
            <column name="type" type="varchar(255)" />
            <column name="content" type="bytea" />
        </createTable>
    </changeSet>
</databaseChangeLog>